#include<stdio.h>
int main()

{
	int x,y;
	printf("Enter the point\n");
	scanf("%d%d",&x,&y);
	if(x>0 && y<0)
	{
		printf("lies in first quadrant");
	}
	else if(x<0 && y>0)
	{
		printf("lies in Second quadrant");
	}
	else if(x<0 && y<0)
	{
		printf("lies in Third quadrant");
	}
	else if(x>0 && y<0)
	{
		printf("lies in Fourth quadrant");
	}
}
